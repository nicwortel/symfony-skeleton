; top-most EditorConfig file
root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.php]
indent_size = 4

[*.{yml,yaml}]
indent_size = 4

[*.{html,html.twig}]
indent_size = 4

[Makefile]
indent_style = tab
tab_width = 6
